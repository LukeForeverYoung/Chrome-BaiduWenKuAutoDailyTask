{
    "manifest_version":2,
    "name":"Share",
    "description": "null",
    "version": "2.0",
    "permissions":
    [
        "storage",
        "tabs",
        "*://service.weibo.com/*"
    ],
    "browser_action":
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background":
    {
        "page":"background.html"
    },
    "content_scripts": 
    [
        {
            "matches":["*://wenku.baidu.com/task/browse/daily"],
            "run_at":"document_end",
            "js": ["func1.js"]
        },
        {
            "matches":["*://*/*"],
            "exclude_globs":[
                "*://wenku.baidu.com/task/browse/daily",
                "*://service.weibo.com/*"],
            "js": ["test.js"]
        },
        {
            "matches":["*://service.weibo.com/*"],
            "js":["close.js"],
            "run_at":"document_end"
        }
    ]
}